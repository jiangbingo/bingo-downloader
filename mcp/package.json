{
  "name": "@bingo/downloader-mcp",
  "version": "1.0.0",
  "description": "MCP Server for Bingo Video Downloader - Download videos from 1000+ websites",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "bingo-downloader-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "video-downloader",
    "yt-dlp",
    "youtube",
    "bilibili",
    "tiktok"
  ],
  "author": "jiangbingo",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/pino": "^7.0.4",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
